
<div class="container">

	<h1>What would you like to ask?</h1>

	<div class="new-q-btn">
			{{new-question saveQuestion="saveQuestion"}}
	</div>

	<br>
	<h4>Total Questions: {{model.length}}</h4>

	<ul id="all-qs">
	  {{#each model as |question|}}
	    {{question-tile question=question}}
			<br>
	  {{/each}}
	</ul>

</div>
